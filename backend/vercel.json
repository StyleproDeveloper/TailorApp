{
  "version": 2,
  "buildCommand": "echo 'No build step required for serverless functions'",
  "outputDirectory": null,
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}
